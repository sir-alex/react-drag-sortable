{
  "name": "react-drag-sortable",
  "version": "1.0.5",
  "description": "React drag & drop highly customizable sortable list component",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --devtool eval --progress --colors --content-base demo/build --hot --inline",
    "build:demo": "webpack -p --content-base demo/build",
    "compile": "node_modules/.bin/babel -d lib/ src/",
    "prepublish": "npm run compile && npm run build:demo"
  },
  "repository": {
    "type": "git",
    "url": "http://git@github.com/tom-s/react-drag-sortable.git"
  },
  "keywords": [
    "react",
    "js",
    "interact",
    "drag",
    "sort",
    "list"
  ],
  "author": "tom-s",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.15.5",
    "file-loader": "^0.8.4",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^1.0.3",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.9",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  },
  "dependencies": {
    "interactjs": "^1.3.4",
    "lodash": "^4.17.11"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  }
